{
  "name": "starhackit-eks-lean",
  "projectName": "starhackit",
  "version": "1.27.0",
  "private": true,
  "description": "Deploying starhackit on AWS EKS without the Load Balancer Controller",
  "main": "index.js",
  "scripts": {
    "graph-target": "gc graph",
    "graph-live": "gc list --graph -a --default-exclude --types-exclude Certificate --types-exclude Route53Domain --types-exclude NetworkInterface",
    "e2e": "gc d -f && gc d && gc l && gc a -f && gc a && gc d -f && gc d && gc l -o",
    "dot": "dot -Tsvg modules.dot -o modules.svg"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grucloud/core": "^1.27.0",
    "@grucloud/module-aws-certificate": "^1.27.0",
    "@grucloud/module-aws-eks": "^1.27.0",
    "@grucloud/module-aws-load-balancer": "^1.27.0",
    "@grucloud/module-aws-vpc": "^1.27.0",
    "@grucloud/provider-aws": "^1.27.0",
    "@grucloud/provider-k8s": "^1.27.0",
    "axios": "^0.21.1",
    "rubico": "^1.6.23"
  }
}
