{
  "name": "@grucloud/docker-cli",
  "version": "1.17.4",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "docker:build": "docker build -t grucloud-cli .",
    "docker:tag": "docker tag grucloud-cli fredericheem/grucloud-cli:v1.17.3",
    "docker:push": "docker push fredericheem/grucloud-cli:v1.17.3",
    "gc:list:aws": "docker run --mount type=bind,source=$PWD/volume,target=/app/output --env-file=aws.env grucloud-cli list -p aws --json output/gc-list.json --graph --all --dot-file output/gc.dot",
    "gc:info:aws": "docker run --env-file=aws.env grucloud-cli info",
    "gc:version:aws": "docker run --env-file=aws.env grucloud-cli --version",
    "gc:info:gcp": "docker run  --mount type=bind,source=$PWD/volume,target=/app/output  grucloud-cli info --provider google",
    "gc:version:gcp": "docker run  grucloud-cli --version",
    "gc:list:gcp": "docker run --mount type=bind,source=$PWD/volume,target=/app/output  grucloud-cli list -p google --json output/gc-list.json --graph --all --dot-file output/gc.dot"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grucloud/core": "^1.17.4",
    "@grucloud/provider-aws": "^1.17.4",
    "@grucloud/provider-azure": "^1.17.4",
    "@grucloud/provider-google": "^1.17.4",
    "@grucloud/provider-k8s": "^1.17.4"
  }
}
