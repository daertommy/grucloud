{
  "name": "@grucloud/docker-aws",
  "version": "1.16.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "docker:build": "docker build -t grucloud-cli .",
    "docker:tag": "docker tag grucloud-cli fredericheem/grucloud-cli:v1.16.0",
    "docker:push": "docker push fredericheem/grucloud-cli:v1.16.0",
    "gc:list": "docker run --mount type=bind,source=$PWD/volume,target=/app/output --env-file=aws.env grucloud-cli list --json output/gc-list.json --graph --all --dot-file output/gc.dot",
    "gc:info": "docker run --env-file=aws.env grucloud-cli info",
    "gc:version": "docker run --env-file=aws.env grucloud-cli --version"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grucloud/core": "^1.16.0",
    "@grucloud/provider-aws": "^1.16.0",
    "@grucloud/provider-azure": "^1.16.0",
    "@grucloud/provider-google": "^1.16.0",
    "@grucloud/provider-k8s": "^1.16.0"
  }
}
