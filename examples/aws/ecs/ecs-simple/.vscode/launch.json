{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "E2E Test",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["test"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "port": 9229
    },
    {
      "name": "Graph",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": ["graph"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "port": 9229
    },
    {
      "name": "List",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": [
        "list",
        "-a",
        "-g",
        "--json",
        "artifacts/inventory.json",
        "--default-exclude",
        "-e",
        "Registry",
        "-e",
        "Key",
        "-e",
        "Certificate",
        "-e",
        "Domain"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "port": 9229
    },
    {
      "name": "GenCode",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": [
        "gencode",
        "--input",
        "artifacts/inventory.json",
        "--outputConfig",
        "config.js",
        "--outputCode",
        "iac.js"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "port": 9229
    },
    {
      "name": "Plan",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": ["plan"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "port": 9229
    },
    {
      "name": "Apply",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": ["apply", "-f"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "port": 9229
    },
    {
      "name": "Destroy",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": ["destroy", "-f"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "port": 9229
    },
    {
      "name": "Destroy AutoScalingGroup",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": ["destroy", "-f", "-t", "AutoScalingGroup"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "port": 9229
    },
    {
      "name": "Destroy All",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": ["destroy", "-f", "-a"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "port": 9229
    },
    {
      "name": "onDeployed",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": ["run", "--onDeployed"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "port": 9229
    },
    {
      "name": "onDestroyed",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": ["run", "--onDestroyed"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "port": 9229
    },
    {
      "name": "Target Graph",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": ["graph"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "port": 9229
    }
  ]
}
