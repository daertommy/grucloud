{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "EC2 Graph",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": ["graph"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}/example",
      "port": 9229
