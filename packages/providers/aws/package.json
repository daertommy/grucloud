    "test-serverless": "dotenv -e ../../../secrets/default.env -- mocha --exit '../../../examples/aws/serverless-patterns/**/iac.test.js' ",
    "test-wafv2": "dotenv -e ../../../secrets/default.env -- mocha --exit '../../../examples/aws/WAFv2/**/iac.test.js' ",
    "test-modules": "dotenv -e ../../../secrets/default.env -- mocha --exit '../../modules/aws/**/iac.test.js' ",
    "test-samples": "dotenv -e ../../../secrets/default.env -- mocha --exit '../../../examples/aws/aws-samples/**/iac.test.js' ",
    "coverage": "nyc npm run test-all",
    "coverage-examples": "nyc npm run test-examples",
    "coverage-modules": "nyc npm run test-modules",
    "test-all": "npm run test && npm run test-examples && npm run test-modules"
