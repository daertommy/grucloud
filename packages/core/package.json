{
  "name": "@grucloud/core",
  "version": "7.0.0",
  "description": "GruCloud core, Infrastructure as Code",
  "public": true,
  "main": "index.js",
  "bin": {
    "gc": "./cli/cliEntry.js"
  },
  "homepage": "https://www.grucloud.com",
  "scripts": {
    "test:mock": "NODE_PATH=. mocha  './cli/**/*.test.js'  './test/**/*.test.js'",
    "test": "npm run test:mock"
  },
  "keywords": [
    "cloud",
    "infrastructure as code"
  ],
  "author": "Frederic Heem",
  "license": "ISC",
  "devDependencies": {
    "mocha": "^8.2.1",
    "mocha-junit-reporter": "^2.0.0",
    "sinon": "^9.2.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "change-case": "^4.1.2",
    "cli-table3": "^0.6.0",
    "colors": "1.4.0",
    "commander": "^7.0.0",
    "deep-object-diff": "^1.1.0",
    "diff": "^5.0.0",
    "duration": "^0.2.2",
    "eslint": "^8.6.0",
    "eslint-plugin-autofix": "^1.1.0",
    "fs-extra": "^10.0.0",
    "generate-password": "^1.7.0",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.20",
    "md5-file": "^5.0.0",
    "pluralize": "^8.0.0",
    "prettier": "^2.3.2",
    "prompts": "^2.4.0",
    "remedial": "^1.0.8",
    "rubico": "^1.7.3",
    "rxjs": "^6.6.3",
    "shelljs": "^0.8.4",
    "shelljs-live": "0.0.5",
    "spinnies": "^0.5.1",
    "url-join": "^4.0.1",
    "winston": "^3.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grucloud/grucloud"
  },
  "nyc": {
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "cli/cliEntry.js",
      "**/*.spec.js",
      "**/*test*.js",
      "**/test/*.js",
      "**/test/config/*.js",
      "cli/test/infra/*.js"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary"
    ],
    "all": true
  },
  "np": {
    "yarn": false,
    "branch": "main"
  }
}
