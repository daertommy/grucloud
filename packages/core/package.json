{
  "name": "@grucloud/core",
  "version": "12.6.2",
  "description": "GruCloud core, generate infrastructure code",
  "public": true,
  "main": "index.js",
  "bin": {
    "gc": "./cli/cliEntry.js"
  },
  "homepage": "https://www.grucloud.com",
  "scripts": {
    "test": "node --env-file ../../secrets/default.env  node_modules/mocha/bin/_mocha  './cli/**/*.test.js'  './test/**/*.test.js'",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "cloud",
    "infrastructure as code",
    "iac",
    "AWS",
    "Azure",
    "GCP"
  ],
  "author": "Frederic Heem",
  "license": "ISC",
  "devDependencies": {
    "mocha": "10.2.0",
    "mocha-junit-reporter": "2.2.0",
    "nyc": "15.1.0",
    "sinon": "15.0.4"
  },
  "dependencies": {
    "axios": "1.3.6",
    "change-case": "4.1.2",
    "cli-table3": "0.6.0",
    "colors": "1.4.0",
    "commander": "10.0.1",
    "deep-object-diff": "1.1.9",
    "diff": "5.1.0",
    "duration": "0.2.2",
    "eslint": "8.39.0",
    "eslint-plugin-autofix": "1.1.0",
    "fs-extra": "11.1.1",
    "generate-password": "1.7.0",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "md5-file": "5.0.0",
    "netmask": "2.0.2",
    "pluralize": "8.0.0",
    "prettier": "2.8.8",
    "prompts": "2.4.2",
    "remedial": "1.0.8",
    "rubico": "1.9.6",
    "rxjs": "7.5.7",
    "shelljs": "0.8.5",
    "shelljs-live": "0.0.5",
    "spinnies": "0.5.1",
    "url-join": "4.0.1",
    "winston": "3.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grucloud/grucloud"
  },
  "nyc": {
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "cli/cliEntry.js",
      "**/*.spec.js",
      "**/*test*.js",
      "**/test/*.js",
      "**/test/config/*.js",
      "cli/test/infra/*.js",
      "template/*",
      "coverage/*"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary"
    ],
    "all": true
  },
  "np": {
    "yarn": false,
    "branch": "main"
  }
}
