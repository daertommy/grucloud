{
  "name": "@grucloud/module-k8s-cert-manager",
  "version": "1.10.4",
  "description": "Grucloud module for the k8s cert-manager",
  "main": "iac.js",
  "scripts": {
    "test": "mocha --exit './{,!(node_modules)/**}/*.test.js'",
    "e2e": "gc d -f && gc d && gc l && gc a -f && gc a && gc d -f && gc d && gc l -o",
