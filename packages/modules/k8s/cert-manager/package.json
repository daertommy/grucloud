    "download-manifest": "curl -L -o cert-manager.yaml https://github.com/jetstack/cert-manager/releases/download/v1.0.2/cert-manager.yaml"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grucloud/core": "^1.10.4",
    "@grucloud/provider-k8s": "^1.10.4",
    "rubico": "^1.6.21"
  },
  "devDependencies": {
    "mocha": "^8.3.2"
  }
}
