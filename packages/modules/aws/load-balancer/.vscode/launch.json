{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Load Balancer Module Graph",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": ["graph"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}/example",
      "port": 9229
    },
    {
      "name": "Load Balancer Module List",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "gc",
      "runtimeArgs": ["list", "-g"],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}/example",
      "port": 9229
    }
  ]
}
